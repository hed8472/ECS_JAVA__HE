public class YourSchedule 
{
	public static void main(String[] args) 
	{
		String p1, p2, p3, p4, P1, P2, P3, P4;
		
		p1 = "English 9H   ";
		p2 = "ECS          ";
		p3 = "Math 2 Honors";   
		p4 = "ISPE         ";
		P1 = "Myers        ";
		P2 = "Mauro        ";
		P3 = "Happ         ";
		P4 = "Thornton     ";
		
		
		System.out.println("David's Class Schedule:");
		System.out.println("");
		System.out.println("----------------------------------------------------");
		System.out.println("|Per 1|     "+p1+"   |     "+ P1+"    |");
		System.out.println("|Per 2|     "+p2+"   |     "+ P2+"    |");
		System.out.println("|Per 3|     "+p3+"   |     "+ P3+"    |");
		System.out.println("|Per 4|     "+p4+"   |     "+ P4+"    |");
	}
}