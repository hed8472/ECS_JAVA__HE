package assignment_7;
import java.util.Scanner;
public class CountingMachine {
	public static void main( String[] args )
	{
		int x;
		Scanner keyboard = new Scanner (System.in);

		System.out.print( "Count to: ");
		x = keyboard.nextInt();
		System.out.println();
		for ( int n = 0 ; n <= x ; n = n++ )
		{
			System.out.println(n + " ");
		}
		keyboard.close();
	}
}
