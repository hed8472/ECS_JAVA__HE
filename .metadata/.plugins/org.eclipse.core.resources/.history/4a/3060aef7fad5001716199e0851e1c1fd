public class UsingVariables 
{
	public static void main(String[] args) 
	{
		int room_number;
		double e;
		String my_class;
		
		room_number = 113;
		e = 2.71828;
		my_class = "Computer Science";
		
		System.out.println("This is room # " + room_number);
		System.out.println("e is close to " + e);
		System.out.println("I am learning a bit about  " + my_class);
	}
}